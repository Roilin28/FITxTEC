{
  "expo": {
    "name": "fitxtec-rn",
    "slug": "fitxtec-rn",
    "owner": "irojas44",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fitxtecrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.fitxtecrn"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "google": {
        "expoClientId": "152331365997-frfpmald72k4kl1l6kgbpjtfkdm32a0e.apps.googleusercontent.com",
        "androidClientId": "REPLACE_ME_ANDROID_CLIENT_ID.apps.googleusercontent.com",
        "iosClientId": "REPLACE_ME_IOS_CLIENT_ID.apps.googleusercontent.com",
        "webClientId": "152331365997-frfpmald72k4kl1l6kgbpjtfkdm32a0e.apps.googleusercontent.com"
      },
      "demoAuth": true
    }
  }
}
